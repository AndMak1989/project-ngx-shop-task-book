{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "components": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "sass"
        }
      },
      "root": "1-components",
      "sourceRoot": "1-components/1-shop-card/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/components",
            "index": "shared/index.html",
            "main": "1-components/1-shop-card/src/main.ts",
            "polyfills": "shared/polyfills.ts",
            "tsConfig": "1-components/1-shop-card/src/tsconfig.app.json",
            "aot": true,
            "assets": [
              "1-components/1-shop-card/src/favicon.ico",
              "1-components/1-shop-card/src/assets"
            ],
            "styles": ["shared/styles.sass"]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "1-components/1-shop-card/src/environments/environment.ts",
                  "with": "1-components/1-shop-card/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "components:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "components:build:production"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "1-components/1-shop-card/src/test.ts",
            "polyfills": "shared/polyfills.ts",
            "tsConfig": "1-components/1-shop-card/src/tsconfig.spec.json",
            "karmaConfig": "shared/karma.conf.js",
            "assets": [
              "1-components/1-shop-card/src/favicon.ico",
              "1-components/1-shop-card/src/assets"
            ],
            "styles": ["shared/styles.sass"]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "1-components/1-shop-card/src/tsconfig.app.json",
              "1-components/1-shop-card/src/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
