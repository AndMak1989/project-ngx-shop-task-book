{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "components": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "sass"
        }
      },
      "root": "projects/components/1-template-syntax-and-binding",
      "sourceRoot": "projects/components/1-template-syntax-and-binding/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/components",
            "index": "projects/components/1-template-syntax-and-binding/src/index.html",
            "main": "projects/components/1-template-syntax-and-binding/src/main.ts",
            "polyfills": "projects/shared/polyfills.ts",
            "tsConfig": "projects/components/1-template-syntax-and-binding/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/components/1-template-syntax-and-binding/src/favicon.ico",
              "projects/components/1-template-syntax-and-binding/src/assets"
            ],
            "styles": ["projects/shared/styles.sass"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/components/1-template-syntax-and-binding/src/environments/environment.ts",
                  "with": "projects/components/1-template-syntax-and-binding/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "components:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "components:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "components:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/components/1-template-syntax-and-binding/src/test.ts",
            "polyfills": "projects/shared/polyfills.ts",
            "tsConfig": "projects/components/1-template-syntax-and-binding/tsconfig.spec.json",
            "karmaConfig": "projects/shared/karma.conf.js",
            "assets": [
              "projects/components/1-template-syntax-and-binding/src/favicon.ico",
              "projects/components/1-template-syntax-and-binding/src/assets"
            ],
            "styles": ["projects/shared/styles.sass"],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/components/1-template-syntax-and-binding/tsconfig.app.json",
              "projects/components/1-template-syntax-and-binding/tsconfig.spec.json",
              "projects/components/1-template-syntax-and-binding/e2e/tsconfig.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/components/1-template-syntax-and-binding/e2e/protractor.conf.js",
            "devServerTarget": "components:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "components:serve:production"
            }
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "e6f3bf19-a5e1-4a2d-a815-789e10c813fe"
  },
  "defaultProject": "components"
}
